/**
 * @see https://www.electron.build/configuration/configuration
 */
{
  "appId": "xxldm.vitesse",
  "asar": true,
  "directories": {
    "output": "release"
  },
  "files": [
    "dist"
  ],
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    ],
    "icon": "public/favicon-512x512.png",
    "artifactName": "${productName}_v${version}.${ext}"
  },
  "nsis": {
    "oneClick": false,
    "perMachine": true,
    "allowToChangeInstallationDirectory": true,
    "deleteAppDataOnUninstall": false
  }
}
