{
  "name": "vitesse-electron",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "description": "自用electron模板",
  "author": { "name": "xxldm", "url": "https://github.com/xxldm" },
  "main": "dist/electron/main/index.js",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vite build --base ./ && electron-builder --publish onTag",
    "build:dir": "vite build --base ./ && electron-builder --dir",
    "build:vite": "vite build",
    "build:electron": "electron-builder",
    "preview": "vite preview",
    "preview:build:base": "vite build --base %npm_config_base% && vite preview --base %npm_config_base%",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "@vueuse/head": "^0.7.9",
    "@vueuse/integrations": "^9.1.0",
    "@vueuse/router": "^9.1.0",
    "axios": "^0.27.2",
    "element-plus": "^2.2.14",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.20",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@iconify-json/carbon": "^1.1.7",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3",
    "eslint": "^8.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jsdom": "^20.0.0",
    "pnpm": "^7.8.0",
    "sass": "^1.54.5",
    "taze": "^0.7.6",
    "typescript": "^4.7.4",
    "unocss": "^0.45.8",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-electron": "^0.9.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-pwa": "^0.12.3",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.40.1"
  }
}
